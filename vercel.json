{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites":[
    {
      "source":"/users/",
       "destination":"api/users/"
    },
    {
      "source":"/posts/:id",
       "destination":"api/posts/:id"
    }
  ]
}